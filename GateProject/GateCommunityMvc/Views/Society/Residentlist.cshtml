@model IEnumerable<GateCommunityMvc.Models.Tblresident>

@{
    ViewData["Title"] = "Residentlist";
    Layout = "~/Views/Society/_Layout.cshtml";
}

<h1>Residentlist</h1>


<table id="reslist" class="table table-striped table-bordered">
    <thead>
        <tr>
           
            <th>
               Wing Name
            </th>
            <th>
              Flat number
            </th>
            <th>
               Full name
            </th>
            
            <th>
              Contact no.
            </th>
            <th>
              Gender
            </th>
            <th>
               A/c Status

            </th>
            <th>
             Doc type
            </th>
            <th>
              Docu
            </th>
           
            <th>
              Profile
            </th>
            <th>
               Gate Pass
            </th>
            
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @item.Wingmodel.WingName
            </td>
            <td>
                    @item.Flatmodel.flatno
            </td>
            <td>
                    @item.firstname
                    @item.lastname
            </td>
            
            <td>
                    @item.phoneNumber
            </td>
            <td>
                    @item.gender
            </td>
            <td>
                @item.status
            </td>
            <td>
                    @item.doc_type
            </td>
            <td>
                    <a href="@Url.Action("OpenFile", "Resident", new { id = item.residentId })">
                        <i class="fas fa-eye"></i> View File
                    </a>
            </td>
           
            <td>
                    <img src="~/@item.profilepath" width="50" height="50" alt="Profile Image" />
            </td>
            <td>
              @item.gatepasscode
            </td>
           
        </tr>
}
    </tbody>
</table>
@section res {

    <script src="https://code.jquery.com/jquery-3.7.0.min.js"></script>

    <link href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js" defer></script>
    <script>
        $(document).ready(function () {
            $('#reslist').DataTable();
        });
    </script>

}
