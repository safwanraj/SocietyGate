@model IEnumerable<GateCommunityMvc.Models.Vehicle>

@{
    ViewData["Title"] = "Vehiclelist";
    Layout = "~/Views/Society/_Layout.cshtml";
}

<h1>Vehiclelist</h1>


<table id="vehiclelist" class="table table-striped table-bordered">

    <thead>
        <tr>
            <th>
                Resident name
            </th>
            <th>
                wing name
            </th>
            <th>
                flatno
            </th>
            <th>
                Vehicle number
            </th>
            <th>
                vehicle type
            </th>
            <th>
                Manufacturer
            </th>
            <th>
                Contact
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Tblresident.firstname
                    @item.Tblresident.lastname

                </td>
                <td>
                    @item.Wingmodel.WingName
                </td>
                <td>
                    @item.Flatmodel.flatno
                </td>

                <td>
                    @item.vehicleno
                </td>
                <td>
                    @item.vehicletype
                </td>
                <td>
                    @item.manufacturer
                </td>
                <td>
                    @item.Tblresident.phoneNumber
                </td>


            </tr>
        }
    </tbody>
</table>
@section veh {

    <script src="https://code.jquery.com/jquery-3.7.0.min.js"></script>

    <link href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js" defer></script>
    <script>
        $(document).ready(function () {
            $('#vehiclelist').DataTable();
        });
    </script>

}