@model IEnumerable<GateCommunityMvc.Models.Entryexit>

@{
    ViewData["Title"] = "Entryexitlist";
    Layout = "~/Views/Guard/_Layout.cshtml";
}

<h1>Entryexitlist</h1>

<table id="exlist" class="table table-striped table-bordered">
    <thead>
        <tr>
          
            <th>
                Visitor name
            </th>
            <th>
                Type
            </th>

            <th>
                Entry
            </th>
            <th>Exit</th>
            <th>
                Resident name
            </th>

           

           
          
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @item.vname
                </td>
                <td>
                    @item.type
                </td>
                <td>
                    @item.Entry
                </td>
                <td>
                    @item.Exit
                </td>
                <td>
                    @item.Tblresident.firstname
                    @item.Tblresident.lastname
                </td>
        </tr>
}
    </tbody>
</table>
@section exlist {

    <script src="https://code.jquery.com/jquery-3.7.0.min.js"></script>

    <link href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js" defer></script>
    <script>
        $(document).ready(function () {
            $('#exlist').DataTable();
        });
    </script>

}
